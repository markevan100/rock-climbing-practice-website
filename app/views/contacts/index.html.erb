<h1>Current contacts</h1>
<p>
  <%= link_to "Create new contact", new_contact_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Name</th>
    <th>Email</th>
    <th>Cell</th>
    <th></th>
  </tr>

  <% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.title %></td>
    <td><%= link_to contact.name, contact_path(contact) %></td>
    <td><%= contact.email %></td>
    <td><%= contact.cell %></td>

  </tr>
  <% end %>
</table>
